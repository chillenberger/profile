{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "dist/server",
    "noEmit": false,
    "skipLibCheck": true,
    "allowImportingTsExtensions": false,
    "target": "ES2022",
    "types": [
      "node"
    ]
  },
  "include": [
    "server/**/*"
  ]
}